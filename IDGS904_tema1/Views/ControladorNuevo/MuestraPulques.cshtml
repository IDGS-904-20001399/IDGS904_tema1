@model List<IDGS904_tema1.Models.Productos>

@{
    ViewBag.Title = "MuestraPulques";
}

<h2>MuestraPulques</h2>

<div>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Descripción</th>
                <th>Cantidad</th>
                <th>Producción</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Nombre</td>
                    <td>@item.Descripcion</td>
                    <td>@item.Cantidad</td>
                    <td>@item.Produccion</td>
                </tr>
            }
        </tbody>
    </table>
</div>
